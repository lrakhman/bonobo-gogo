<%if session[:user_id]%>


  <%@surveys.each do |survey|%>
			<div id="survey">
  		<h2 id="title"><a href= <%="/surveys/#{survey.id}"%> ><%=survey.title%></a></h2>
  			<p><%=survey.created_at.in_time_zone("Central Time (US & Canada)").strftime("%m/%d/%Y %I:%M%p")%></p>
  			<p><%=survey.description%><p>
			</div>
    <%end%>



<%end%>

